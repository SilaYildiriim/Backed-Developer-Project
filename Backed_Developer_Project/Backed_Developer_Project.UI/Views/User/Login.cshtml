
@{
    Layout = null;
}

@model LoginDto

<form asp-controller="User" asp-action="Login">
    <div class="form-group">
        <label for="userName">UserName</label>
        <input asp-for="UserName" type="text" class="form-control" id="userName"  placeholder="Enter UserName">
    </div>
    <div class="form-group">
        <label for="password">Password</label>
        <input asp-for="Password" type="password" class="form-control" id="password" placeholder="Password">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

